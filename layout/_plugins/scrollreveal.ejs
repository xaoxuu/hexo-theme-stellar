<script defer src="<%- conf.js %>"></script>
<script>
  utils.initPlugin(() => {
    const els = document.querySelectorAll('.slide-up');
    if (els.length > 0) {
      const slideUp = {
        distance: `<%- conf.distance %>`,
        duration: `<%- conf.duration %>`,
        interval: `<%- conf.interval %>`,
        scale: `<%- conf.scale %>`,
        opacity: 0,
        easing: "ease-out"
      };
      ScrollReveal().reveal('.slide-up', { ...slideUp });
    }
  }, 'scrollreveal');
</script>
